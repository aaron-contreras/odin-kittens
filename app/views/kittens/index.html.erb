<table>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th>Cuteness</th>
    <th>Softness</th>
  </tr>

  <% @kittens.each do |kitten| %>
    <tr>
      <td><%= kitten.name %></td>
      <td><%= kitten.age %></td>
      <td><%= kitten.cuteness %></td>
      <td><%= kitten.softness %></td>
      <td>
        <%= link_to 'Delete', kitten_path(kitten), method: :delete %>
      </td>
    </tr>
  <% end %>
</table>
